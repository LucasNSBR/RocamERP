@model RocamERP.Presentation.Web.ViewModels.BancoViewModel

<div class="form-group">
    @Html.LabelFor(model => model.Nome, new { @class = "col-form-label" })
    <div class="col-12">
        @Html.TextBoxFor(e => e.Nome, new { @class = "form-control" })
        @Html.ValidationMessageFor(e => e.Nome, null, new { @class = "small col-12 text-right text-danger pt-2" })
    </div>
</div>